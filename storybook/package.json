{
  "name": "dialtone-vue-storybook",
  "version": "1.0.0",
  "description": "Storybook for Dialtone Vue",
  "scripts": {
    "storybook": "NODE_PATH=./node_modules start-storybook -c ../.storybook --no-version-updates --ci -p 9010",
    "build": "NODE_PATH=./node_modules build-storybook -c ../.storybook -o compiled",
    "storybook-a11y-test": "NODE_PATH=./node_modules node ./scripts/storybook-a11y-test.js"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@shopify/storybook-a11y-test": "^0.5.0",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-backgrounds": "^6.4.19",
    "@storybook/addon-controls": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/source-loader": "^6.4.19",
    "@storybook/theming": "^6.4.19",
    "@storybook/vue3": "^6.4.19",
    "@vue/compiler-sfc": "^3.2.31",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "generate-file-webpack-plugin": "^1.0.1",
    "vue-loader": "^17.0.0"
  },
  "dependencies": {
    "focus-visible": "^5.2.0",
    "postcss-focus-visible": "^6.0.3",
    "vue": "^3.2.22"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 16"
  },
  "volta": {
    "node": "16"
  }
