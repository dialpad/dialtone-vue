{
  "name": "@dialpad/dialtone-vue",
  "version": "2.80.0",
  "description": "Vue component library for Dialpad's design system Dialtone",
  "scripts": {
    "build": "npm run clean && npm run build:main && npm run build:documentation",
    "build:main": "vite build",
    "lint": "run-s lint:code lint:docs",
    "build:documentation": "node scripts/build-docs.js",
    "clean": "rm -rf ./dist",
    "install:all": "npm install && npm install --prefix generator-dialtone-vue",
    "lint-staged:code": "eslint --ext .js,.vue --ignore-path .gitignore --fix",
    "lint-staged:docs": "markdownlint --fix",
    "lint:code": "eslint --ext .js,.vue --ignore-path .gitignore",
    "lint:docs": "markdownlint '**/*.mdx'",
    "prepublishOnly": "npm run build",
    "pretest": "npm run clean",
    "release": "semantic-release --no-ci --extends ./release-local.config.cjs",
    "start": "STORYBOOK_ENV=development npm run storybook",
    "storybook": "storybook dev -p 9010",
    "storybook:build": "storybook build && touch ./storybook-static/.nojekyll",
    "storybook:a11y-test": "node ./.storybook/storybook-a11y-test.cjs",
    "test": "jest",
    "test:one": "jest -t",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "CHANGELOG.json"
  ],
  "dependencies": {
    "@dialpad/dialtone-icons": "^0.6.1",
    "@linusborg/vue-simple-portal": "^0.1.5",
    "@tiptap/extension-document": "^2.0.2",
    "@tiptap/extension-paragraph": "^2.0.2",
    "@tiptap/extension-text": "^2.0.2",
    "@tiptap/pm": "^2.0.2",
    "@tiptap/vue-2": "^2.0.2",
    "@vue/vue2-jest": "^29.2.3",
    "emoji-regex": "^10.2.1",
    "emoji-toolkit": "^6.6.0",
    "seedrandom": "^3.0.5",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.21.4",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@dialpad/conventional-changelog-angular": "^1.1.1",
    "@dialpad/dialtone": "^7.25.1",
    "@dialpad/semantic-release-changelog-json": "^1.0.0",
    "@percy/cli": "^1.24.0",
    "@percy/storybook": "^4.3.6",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@shopify/storybook-a11y-test": "^1.1.2",
    "@storybook/addon-a11y": "^7.0.6",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-styling": "^1.0.1",
    "@storybook/blocks": "^7.0.6",
    "@storybook/manager-api": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "^7.0.6",
    "@storybook/vue": "^7.0.6",
    "@storybook/vue-vite": "^7.0.6",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/test-utils": "1.3",
    "axe-core": "^4.6.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.5.0",
    "chai": "^4.3.7",
    "eslint": "8.33",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "less": "^4.1.3",
    "lint-staged": "^13.1.0",
    "macos-release": "^3.1.0",
    "markdownlint": "^0.27.0",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^19.0.5",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.21",
    "storybook": "^7.0.6",
    "vite": "^4.2.1",
    "vue": "^2.7.14",
    "vue-cli-plugin-webpack-bundle-analyzer": "~4.0.0",
    "vue-docgen-api": "^4.56.4",
    "vue-jest": "^3.0.7",
    "yo": "^4.3.1",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "@dialpad/dialtone": ">=7.25",
    "vue": ">=2.6"
  },
  "bugs": {
    "email": "dialtone@dialpad.com"
  },
  "contributors": [
    {
      "name": "Brad Paugh",
      "email": "brad.paugh@dialpad.com",
      "url": "https://github.com/braddialpad"
    },
    {
      "name": "Francis Rupert",
      "email": "francis.rupert@dialpad.com",
      "url": "https://github.com/francisrupert"
    },
    {
      "name": "Julio Ortega",
      "email": "julio.ortega@dialpad.com",
      "url": "https://github.com/juliodialpad"
    },
    {
      "name": "Ignacio Ropolo",
      "email": "ignacio.ropolo@dialpad.com",
      "url": "https://github.com/iropolo"
    }
  ],
  "engineStrict": true,
  "engines": {
    "node": ">= 16"
  },
  "type": "module",
  "main": "./dist/dialtone-vue.cjs",
  "module": "./dist/dialtone-vue.js",
  "exports": {
    ".": {
      "import": "./dist/dialtone-vue.js",
      "require": "./dist/dialtone-vue.cjs"
    },
    "./emoji": {
      "import": "./dist/emoji.js",
      "require": "./dist/emoji.cjs"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "./scripts/commit-msg"
  },
  "homepage": "https://vue.dialpad.design",
  "keywords": [
    "Dialpad",
    "Dialtone",
    "Dialtone Vue",
    "Design System",
    "Components",
    "Component Library",
    "Vue"
  ],
  "license": "MIT",
  "repository": "git@github.com:dialpad/dialtone-vue.git",
  "volta": {
    "node": "16"
  }
}
